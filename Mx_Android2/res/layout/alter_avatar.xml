<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/alter_avatar_album"
        style="@style/information_sex_popwindow_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/album" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/wealthcenter_content_line_height"
        android:background="@color/login_head_line_color" />

    <TextView
        android:id="@+id/alter_avatar_cancel"
        style="@style/information_sex_popwindow_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/cancel" />

    
</LinearLayout>